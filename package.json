{
  "name": "@acumensolutions/acu-pack",
  "description": "SFDX CLI Extensions by Acumen Solutions Inc.",
  "version": "1.0.0",
  "author": "Acumen Solutions Inc.",
  "bugs": {
    "url": "https://bitbucket.org/acumensolutions/acu-pack/jira"
  },
  "dependencies": {
    "@oclif/command": "^1.8.0",
    "@oclif/config": "^1.17.0",
    "@oclif/errors": "^1.3.3",
    "@salesforce/command": "^2",
    "@salesforce/core": "^2.9.4",
    "@salesforce/kit": "^1.3.2",
    "@salesforce/ts-sinon": "^1.2.2",
    "await-spawn": "^3.0.0",
    "glob": "^7.1.6",
    "md5-file": "^5.0.0",
    "sfdx-cli": "^7.72.0",
    "tslib": "^1",
    "xlsx": "^0.16.6",
    "xml2js": "^0.4.23"
  },
  "devDependencies": {
    "@oclif/dev-cli": "^1",
    "@oclif/plugin-help": "^2",
    "@oclif/test": "^1",
    "@salesforce/dev-config": "^1.4.1",
    "@types/chai": "^4",
    "@types/mocha": "^5",
    "@types/node": "^10",
    "chai": "^4",
    "globby": "^8",
    "mocha": "",
    "nyc": "^14",
    "rimraf": "^3.0.2",
    "ts-node": "^8",
    "tslint": "^5"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "files": [
    "/compiled",
    "/messages",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "homepage": "https://bitbucket.org/acumensolutions/acu-pack",
  "keywords": [
    "sfdx-plugin"
  ],
  "license": "MIT",
  "oclif": {
    "commands": "./compiled/commands",
    "bin": "sfdx",
    "topics": {
      "merge": {
        "description": "Merges SFDX Package XML files."
      },
      "profile": {
        "description": "Builds a Profile-centric SFDX Package XML file."
      },
      "build": {
        "description": "Builds a package based on metadata configured in a specific Org."
      }
    },
    "devPlugins": [
      "@oclif/plugin-help"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://Michael_Malling@bitbucket.org/acumensolutions/acu-pack.git"
  },
  "scripts": {
    "lint": "tslint --project . --config tslint.json --format stylish",
    "postpack": "rimraf oclif.manifest.json",
    "posttest": "tslint -p test -t stylish",
    "prepack": "rimraf compiled && tsc -b && oclif-dev manifest && oclif-dev readme",
    "test": "mocha -r ts-node/register \"test/**/*.test.ts\"",
    "version": "oclif-dev readme && git add README.md"
  },
  "main": "index.js",
  "directories": {
    "test": "test"
  }
}
